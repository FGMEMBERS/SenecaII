                    <y>4</y>
                    <w>12</w>
                    <h>10</h>
                    <repeatable>0</repeatable>
                    <binding>
                        <command>property-toggle</command>
                        <property>/instrumentation/audio-panel/dme</property>
                    </binding>
                </action>

                <!-- ADF Button -->
                <action>
                    <name>ADFButton</name>
                    <button>0</button>
                    <x>42</x>
                    <y>4</y>
                    <w>12</w>
                    <h>10</h>
                    <repeatable>0</repeatable>
                    <binding>
                        <command>property-toggle</command>
                        <property>/instrumentation/adf[0]/ident-audible</property>
