                    </binding>
                </action>

                <!--CREW Button -->
                <action>
                    <name>CREWButton</name>
                    <button>0</button>
                    <x>37</x>
                    <y>-14</y>
                    <w>17</w>
                    <h>7</h>
                    <repeatable>0</repeatable>
                    <binding>
                        <command>property-toggle</command>
                        <property>/instrumentation/audio-panel/crew</property>
                    </binding>
                </action>

                <!--TEST Button -->
                <action>
                    <name>TESTButton</name>
                    <button>0</button>
                    <x>63</x>
                    <y>5</y>
                    <w>12</w>
                    <h>7</h>
                    <repeatable>0</repeatable>
                    <binding>
                        <command>property-toggle</command>
                        <property>/instrumentation/audio-panel/test</property>
                    </binding>
                    <mod-up>
                        <binding>
                            <command>property-assign</command>
                            <property>/instrumentation/audio-panel/test</property>
                            <value type="bool">0</value>
                        </binding>
                    </mod-up>
                </action>

            </actions>

        </instrument>
    </instruments>
</PropertyList>
