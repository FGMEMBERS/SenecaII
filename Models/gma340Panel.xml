                    <button>0</button>
                    <x>-42</x>
                    <y>-14</y>
                    <w>12</w>
                    <h>17</h>
                    <repeatable>0</repeatable>
                    <binding>
                        <command>property-assign</command>
                        <property>/instrumentation/audio-panel/com-mic[0]</property>
