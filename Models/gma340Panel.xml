                    </binding>
                    <binding>
                        <command>property-assign</command>
                        <property>/instrumentation/audio-panel/com-mic[1]</property>
                        <value>0</value>
                    </binding>
                    <binding>
                        <command>property-assign</command>
                        <property>/instrumentation/audio-panel/com-mic[2]</property>
                        <value>0</value>
                    </binding>
                </action>

                <!-- COM2MIC Button -->
                <action>
                    <name>COM2MICButton</name>
                    <button>0</button>
                    <x>-29</x>
                    <y>-14</y>
                    <w>12</w>
                    <h>17</h>
                    <repeatable>0</repeatable>
                    <binding>
                        <command>property-assign</command>
                        <property>/instrumentation/audio-panel/com-mic[1]</property>
                        <value>1</value>
                    </binding>
                    <binding>
                        <command>property-assign</command>
                        <property>/instrumentation/audio-panel/com-mic[0]</property>
                        <value>0</value>
                    </binding>
                    <binding>
                        <command>property-assign</command>
                        <property>/instrumentation/audio-panel/com-mic[2]</property>
                        <value>0</value>
                    </binding>
                </action>

