                <action>
                    <name>COM3MICButton</name>
                    <button>0</button>
                    <x>-16</x>
                    <y>-14</y>
                    <w>12</w>
                    <h>17</h>
                    <repeatable>0</repeatable>
                    <binding>
                        <command>property-assign</command>
                        <property>/instrumentation/audio-panel/com-mic[2]</property>
                        <value>1</value>
                    </binding>
                    <binding>
