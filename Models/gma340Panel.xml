                    </binding>
                    <binding>
                        <command>property-assign</command>
                        <property>/instrumentation/audio-panel/com-mic[0]</property>
                        <value>0</value>
                    </binding>
                </action>

                <!--SENS Button -->
                <action>
                    <name>SENSButton</name>
                    <button>0</button>
                    <x>-57</x>
                    <y>-14</y>
                    <w>13</w>
                    <h>8</h>
                    <repeatable>0</repeatable>
                    <binding>
                        <command>property-toggle</command>
