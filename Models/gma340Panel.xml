<PropertyList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../XMLSchemas/panel.xsd">
    <name>KR87Panel</name>
    <background>Aircraft/SenecaII/Instruments-3d/transparent.rgb</background>
    <w>160</w>
    <h>34</h>
    <instruments>
        <instrument>
            <name>panel hotspots</name>
            <x>80</x>
            <y>17</y>
            <w>160</w>
            <h>34</h>
            <w-base>160</w-base>
            <h-base>34</h-base>
            <actions>

                <!-- MKR Button -->
                <action>
                    <name>MKRButton</name>
                    <button>0</button>
                    <x>-56</x>
                    <y>4</y>
                    <w>12</w>
                    <h>10</h>
                    <repeatable>0</repeatable>
                    <binding>
                        <command>property-toggle</command>
                        <property>/instrumentation/marker-beacon/audio-btn</property>
                    </binding>
                </action>

                <!-- NAV1 Button -->
                <action>
                    <name>NAV1Button</name>
                    <button>0</button>
                    <x>3</x>
                    <y>4</y>
                    <w>12</w>
                    <h>10</h>
                    <repeatable>0</repeatable>
                    <binding>
                        <command>property-toggle</command>
                        <property>/instrumentation/nav[0]/ident</property>
                    </binding>
                </action>

                <!-- NAV2 Button -->
                <action>
                    <name>NAV2Button</name>
                    <button>0</button>
                    <x>16</x>
                    <y>4</y>
