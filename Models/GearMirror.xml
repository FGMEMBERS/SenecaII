<?xml version="1.0" ?>
<PropertyList>
    <path>GearMirror.ac</path>
    <animation>
        <type>noshadow</type>
    </animation>
    <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>100</max-m>
    </animation>
    <animation>
        <type>textranslate</type>
        <object-name>Mirror</object-name>
        <property>gear/gear[0]/position-norm</property>
        <factor>0.5</factor>
        <step>1</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
</PropertyList>
