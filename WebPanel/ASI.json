{ 
    "src":    "ASI.svg",
    "target": "Position1",

    "animations": [
      {
        "element": "#Needle",
        "type":    "transform",
        "transforms": [{
          "type": "rotate",
          "property": "/instrumentation/airspeed-indicator/indicated-speed-kt",
          "interpolation": "../Models/AirspeedInterpolation.xml",
          "x": 256,
          "y": 256
        }]
      },
      {
        "element": "#TAS",
        "type":    "transform",
        "transforms": [{
          "type": "rotate",
          "property": "/instrumentation/airspeed-indicator/tas-face-rotation",
          "scale": -1,
          "x": 256,
          "y": 256
        }]
      }
    ]
}
