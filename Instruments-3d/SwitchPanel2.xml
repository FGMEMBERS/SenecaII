<?xml version="1.0" encoding="UTF-8"?>
<!-- 
Switch Panel center row
-->
<PropertyList>
    <name>SwitchPanel1</name>
    <background>Aircraft/b1900d/Panel/transparent-bg.rgb</background>
    <w>218</w>
    <h>49</h>
    <instruments>
        <instrument>
            <name>hotspots</name>
            <x>109</x>
            <y>25</y>
            <w>218</w>
            <h>50</h>
            <w-base>218</w-base>
            <h-base>50</h-base>
            <layers/>
            <actions>
                <action>
                    <name>LandingLight</name>
                    <button>0</button>
                    <x>-100</x>
                    <y>-23</y>
                    <w>20</w>
                    <h>46</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/lighting/landing-lights</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>
                <action>
                    <name>TaxiLight</name>
                    <button>0</button>
                    <x>-70</x>
                    <y>-23</y>
                    <w>20</w>
                    <h>46</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/lighting/taxi-light</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>
                <action>
                    <name>NavLights</name>
                    <button>0</button>
                    <x>-40</x>
                    <y>-23</y>
                    <w>20</w>
                    <h>46</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/lighting/nav-lights</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>
                <action>
                    <name>AntiCollLight.Fin</name>
                    <button>0</button>
                    <x>-10</x>
                    <y>-23</y>
                    <w>10</w>
                    <h>46</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/lighting/beacon</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>
                <action>
                    <name>AntiCollLight.Wing</name>
                    <button>0</button>
                    <x>0</x>
                    <y>-23</y>
                    <w>10</w>
                    <h>46</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/lighting/strobe</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>
                <action>
                    <name>PitotHeat</name>
                    <button>0</button>
                    <x>20</x>
                    <y>-23</y>
                    <w>20</w>
                    <h>46</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/anti-ice/pitot-heat</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>
                <action>
                    <name>FuelPump.L</name>
                    <button>0</button>
                    <x>50</x>
                    <y>-23</y>
                    <w>20</w>
                    <h>46</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/engines/engine[0]/fuel-pump</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>
                <action>
                    <name>FuelPump.R</name>
                    <button>0</button>
                    <x>80</x>
                    <y>-23</y>
                    <w>20</w>
                    <h>46</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/engines/engine[1]/fuel-pump</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>
            </actions>
        </instrument>
    </instruments>
</PropertyList>
