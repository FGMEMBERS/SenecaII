<?xml version="1.0" encoding="UTF-8"?>
<!-- 
Switch Panel top row
-->
<PropertyList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../XMLSchemas/panel.xsd">
    <name>kx165tso1Panel</name>
    <background>Aircraft/b1900d/Panel/transparent-bg.rgb</background>
    <w>158</w>
    <h>50</h>
    <instruments>
        <instrument>
            <name>hotspots</name>
            <x>79</x>
            <y>25</y>
            <w>158</w>
            <h>50</h>
            <w-base>158</w-base>
            <h-base>50</h-base>
            <actions>
                <action>
                    <name>ComFreqToggle</name>
                    <button>0</button>
                    <x>-40</x>
                    <y>-6</y>
                    <w>10</w>
                    <h>5</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/engines/engine[0]/magneto[0]</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>
            </actions>
        </instrument>
    </instruments>
</PropertyList>
