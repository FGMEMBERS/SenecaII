<?xml version="1.0" ?>
<PropertyList>
    <path>DeiceControl.ac</path>
    <animation>
        <type>material</type>
        <object-name>DeiceControl.TailLight.Glas</object-name>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.0</blue>
            <factor-prop>/controls/deice/surface-deice-wingtail-light</factor-prop>
        </emission>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>DeiceControl.SurfaceSwitch</object-name>
        <property>/controls/deice/surface-deice</property>
        <factor>-40</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>-0.00498202</z-m>
        </center>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>DeiceControl.PropSwitch</object-name>
        <property>/controls/deice/prop-deice</property>
        <factor>-40</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>-0.00498202</z-m>
        </center>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>DeiceControl.WindshieldSwitch</object-name>
        <property>/controls/deice/windshield-deice</property>
        <factor>-40</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>-0.00498202</z-m>
        </center>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>DeiceControl.WingLightSwitch.Lever</object-name>
        <property>/controls/deice/wingice-light</property>
        <factor>-60</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.0400982</y-m>
            <z-m>0.0119393</z-m>
        </center>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>DeiceControl.Ampmeter.Needle</object-name>
        <property>/systems/electrical/deice/prop-deice-amps</property>
        <factor>-2</factor>
        <offset-deg>30</offset-deg>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0.00091</x-m>
            <y-m>0.0238379</y-m>
            <z-m>-0.029</z-m>
        </center>
    </animation>
    <animation>
        <type>noshadow</type>
        <object-name>DeiceControl</object-name>
    </animation>
</PropertyList>
