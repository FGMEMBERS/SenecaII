<?xml version="1.0" encoding="UTF-8"?>
<!-- 
Switch Panel center row
-->
<PropertyList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="../../../XMLSchemas/panel.xsd">
    <name>SwitchPanel1</name>
    <background>Aircraft/b1900d/Panel/transparent-bg.rgb</background>
    <w>120</w>
    <h>49</h>
    <instruments>
        <instrument>
            <name>hotspots</name>
            <x>60</x>
            <y>25</y>
            <w>120</w>
            <h>50</h>
            <w-base>120</w-base>
            <h-base>50</h-base>
            
            <actions>
                <action>
                    <name>Master</name>
                    <button>0</button>
                    <x>-51</x>
                    <y>-23</y>
                    <w>20</w>
                    <h>46</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/electric/battery-switch</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>

                <action>
                    <name>Alternator.L</name>
                    <button>0</button>
                    <x>-18</x>
                    <y>-15</y>
                    <w>20</w>
                    <h>20</h>
                    <binding>
                        <command>property-toggle</command>
                        <property>/controls/electric/engine[0]/generator</property>
                        <step>1</step>
                        <min>0</min>
                        <max>1</max>
                        <wrap>true</wrap>
                    </binding>
                </action>
            
            <action>
                <name>Alternator.R</name>
                <button>0</button>
                <x>7</x>
                <y>-15</y>
                <w>20</w>
                <h>20</h>
                <binding>
                    <command>property-toggle</command>
                    <property>/controls/electric/engine[1]/generator</property>
                    <step>1</step>
                    <min>0</min>
                    <max>1</max>
                    <wrap>true</wrap>
                </binding>
            </action>

            </actions>
            
        </instrument>
    </instruments>
</PropertyList>
